{
  "name": "sprout",
  "description": "Frontend for the sprout application.",
  "engines": {
    "node": "^18.17.0 || ^20.3.0 || >=21.0.0"
  },
  "engines-annotation": "Mostly required by sharp which needs a Node-API v9 compatible runtime",
  "private": true,
  "trustedDependencies": [
    "sharp"
  ],
  "trustedDependencies-annotation": "Needed for bun to allow running install scripts",
  "type": "module",
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/express/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "prebuild.gen": "npm run gen",
    "gen.api.client": "tsx gen/api/client.ts",
    "gen.api.helpers": "tsx gen/api/helpers.ts",
    "gen.api": "npm run gen.api.client && npm run gen.api.helpers",
    "gen": "npm run gen.api",
    "deploy": "echo 'Run \"npm run qwik add\" to install a server adapter'",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "node server/entry.express",
    "start": "vite --open --mode ssr",
    "test.unit": "vitest components",
    "test.unit.ui": "vitest --ui components",
    "qwik": "qwik"
  },
  "devDependencies": {
    "@apidevtools/swagger-parser": "^10.1.1",
    "@auth/qwik": "^0.5.4",
    "@builder.io/qwik": "^1.10.0",
    "@builder.io/qwik-city": "^1.10.0",
    "@openapi-ts-rest/core": "^0.1.4",
    "@qwikest/icons": "^0.0.13",
    "@tailwindcss/typography": "^0.5.16",
    "@ts-rest/core": "^3.51.0",
    "@types/compression": "^1.7.2",
    "@types/eslint": "8.56.10",
    "@types/express": "^4.17.19",
    "@types/node": "20.14.11",
    "@types/nunjucks": "^3.2.6",
    "@typescript-eslint/eslint-plugin": "7.16.1",
    "@typescript-eslint/parser": "7.16.1",
    "@vitest/ui": "^0.34.6",
    "autoprefixer": "^10.4.19",
    "compression": "^1.7.4",
    "daisyui": "^4.12.20",
    "dotenv": "^16.3.2",
    "eslint": "8.57.0",
    "eslint-plugin-qwik": "^1.10.0",
    "nunjucks": "^3.2.4",
    "postcss": "^8.4.39",
    "prettier": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.5.4",
    "tailwindcss": "^3.4.6",
    "tsx": "^4.19.2",
    "typescript": "5.4.5",
    "undici": "*",
    "vite": "^5.4.11",
    "vite-tsconfig-paths": "^4.2.1",
    "vitest": "^0.34.6",
    "zod": "^3.24.1"
  },
  "dependencies": {
    "@openid/appauth": "^1.3.2",
    "express": "4.20.0",
    "maplibre-gl": "^4.7.1"
  }
}
